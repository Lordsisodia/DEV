# SISO IDE Analytics Configuration
# Copy these values to your main .env file

# PostHog Analytics (optional - for usage tracking)
VITE_PUBLIC_POSTHOG_KEY=your_posthog_key_here
VITE_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Set to 'true' to enable analytics (requires user consent)
VITE_ANALYTICS_ENABLED=false

# Set to 'true' for offline-only analytics (no external services)
VITE_ANALYTICS_OFFLINE=true